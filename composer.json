{
    "name": "oncology-support/amazon-sms-helper-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle that simplifies sending SMS messages using Amazon SNS",
    "keywords": ["symfony", "sms", "sns", "amazon", "aws"],
    "homepage": "https://github.com/OncologySupport/AmazonSmsHelperBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Ernest Hymel"
        }
    ],
    "support": {
        "issues": "https://github.com/OncologySupport/AmazonSMSHelperBundle/issues",
        "source": "https://github.com/OncologySupport/AmazonSMSHelperBundle/releases"
    },
    "require": {
        "php": ">=8.2",
        "symfony/http-kernel": "^6.0 || ^7.0",
        "symfony/dependency-injection": "^6.0 || ^7.0",
        "symfony/config": "^6.0 || ^7.0",
        "symfony/yaml": "^6.0 || ^7.0",
        "symfony/flex": "^2.3",
        "aws/aws-sdk-php": "^3.316"
    },
    "require-dev": {
        "symfony/framework-bundle": "^6.0 || ^7.0",
        "nyholm/symfony-bundle-test": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "OncologySupport\\AmazonSMSHelper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OncologySupport\\AmazonSMSHelper\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
